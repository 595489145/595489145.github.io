+    padding-top: 80px;
+    height: 100%;
+    opacity: 0;
+    -webkit-transition: all 400ms cubic-bezier(0.32, 1, 0.23, 1) 250ms;
+    transition: all 400ms cubic-bezier(0.32, 1, 0.23, 1) 250ms;
