+  h4 {
+    font-size: 22px;
+    font-weight: 600;
+    color: gray;
+    & + p {
+      margin-top: 10px;
+    }
+  }
+  h5,
+  h6 {
+    font-size: 20px;
+    font-weight: 600;
+    color: gray;
+    & + p {
+      margin-top: 10px;
+    }
+  }
+  // downscale the font a little bit in mobile
+  @media screen and (max-width: 768px) {
+    h1 {
+      font-size: 30px;
