+    h1,
+    h2,
+    h3,
+    h4,
+    h5,
+    h6 {
+      line-height: 2;
+      margin-top: 20px;
+    }
+  }
+  // V1.6 Hux display title by default.
+  .pager li {
+    width: 48%;
+    &.next {
+      float: right;
+    }
+    &.previous {
+      float: left;
